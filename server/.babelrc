{
  "presets": ["@babel/preset-env"],
  "plugins": ["@babel/plugin-proposal-class-properties",
    "@babel/plugin-proposal-optional-chaining",
    ["module-resolver", {
      "root": ["."],
      "alias": {
        "db": "./server/db",
        "models": "./server/models",
        "graphql-container": "./server/graphql",
        "configs": "./server/configs",
        "utils": "./server/utils"
      }
    }]
  ]
}
